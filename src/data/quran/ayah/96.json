    "juz": "30",
    "hizb": "60.5",
    "asbab": "1",
    "audio": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6116",
    "theme": null,
    "text": "seorang hamba ketika dia melaksanakan salat?",
    "notes": null,
