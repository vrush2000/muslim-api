    "juz": "30",
    "hizb": "60.5",
    "asbab": "1",
    "audio": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6119",
    "theme": null,
    "text": "Bagaimana pendapatmu kalau dia mendustakan (kebenaran) dan berpaling (dari keimanan)?",
