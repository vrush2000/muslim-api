    "page": "597",
    "juz": "30",
    "hizb": "60.5",
    "asbab": "1",
    "audio": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6115",
    "theme": null,
    "text": "Tahukah kamu tentang orang yang melarang",
