    "hizb": "60.5",
    "asbab": "1",
    "audio": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6120",
    "theme": null,
    "text": "Tidakkah dia mengetahui bahwa sesungguhnya Allah melihat (segala perbuatannya)?",
