    "asbab": "0",
    "audio": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/5892",
    "theme": "1120",
    "text": "dia akan dihisab dengan pemeriksaan yang mudah",
    "notes": null,
    "audio_partial": "{\"01\":\"https://cdn.equran.id/audio-partial/Abdullah-Al-Juhany/084008.mp3\",\"02\":\"https://cdn.equran.id/audio-partial/Abdul-Muhsin-Al-Qasim/084008.mp3\",\"03\":\"https://cdn.equran.id/audio-partial/Abdurrahman-as-Sudais/084008.mp3\",\"04\":\"https://cdn.equran.id/audio-partial/Ibrahim-Al-Dossari/084008.mp3\",\"05\":\"https://cdn.equran.id/audio-partial/Misyari-Rasyid-Al-Afasi/084008.mp3\",\"06\":\"https://cdn.equran.id/audio-partial/Yasser-Al-Dosari/084008.mp3\"}"
  },
  {
    "id": "5893",
    "surah": "84",
    "ayah": "9",
    "arab": "وَّيَنْقَلِبُ اِلٰٓى اَهْلِهٖ مَسْرُوْرًاۗ",
