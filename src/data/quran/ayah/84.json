    "page": "589",
    "juz": "30",
    "hizb": "59.75",
    "asbab": "0",
    "audio": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/5896",
    "theme": "1120",
    "text": "Dia akan memasuki (neraka) Sa‘ir (yang menyala-nyala).",
