  {
    "id": "5759",
    "surah": "80",
    "ayah": "1",
    "arab": "عَبَسَ وَتَوَلّٰىٓۙ",
    "latin": "‘Abasa wa tawallā.",
    "page": "585",
    "juz": "30",
    "hizb": "59.25",
    "asbab": "1",
    "audio": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/5759",
    "theme": null,
    "text": "Dia (Nabi Muhammad) berwajah masam dan berpaling",
