    "page": "585",
    "juz": "30",
    "hizb": "59.25",
    "asbab": "0",
    "audio": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/5769",
    "theme": null,
    "text": "Sekali-kali jangan (begitu)! Sesungguhnya (ajaran Allah) itu merupakan peringatan.",
    "notes": null,
