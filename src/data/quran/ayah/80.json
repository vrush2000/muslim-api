    "juz": "30",
    "hizb": "59.25",
    "asbab": "0",
    "audio": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/5776",
    "theme": null,
    "text": "Dari apakah Dia menciptakannya?",
    "notes": null,
    "audio_partial": "{\"01\":\"https://cdn.equran.id/audio-partial/Abdullah-Al-Juhany/080018.mp3\",\"02\":\"https://cdn.equran.id/audio-partial/Abdul-Muhsin-Al-Qasim/080018.mp3\",\"03\":\"https://cdn.equran.id/audio-partial/Abdurrahman-as-Sudais/080018.mp3\",\"04\":\"https://cdn.equran.id/audio-partial/Ibrahim-Al-Dossari/080018.mp3\",\"05\":\"https://cdn.equran.id/audio-partial/Misyari-Rasyid-Al-Afasi/080018.mp3\",\"06\":\"https://cdn.equran.id/audio-partial/Yasser-Al-Dosari/080018.mp3\"}"
  },
  {
    "id": "5777",
    "surah": "80",
    "ayah": "19",
    "arab": "مِنْ نُّطْفَةٍۗ خَلَقَهٗ فَقَدَّرَهٗۗ",
    "latin": "Min nuṭfah(tin), khalaqahū fa qaddarah(ū).",
    "page": "585",
    "juz": "30",
    "hizb": "59.25",
    "asbab": "0",
    "audio": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/5777",
    "theme": null,
