    "page": "524",
    "juz": "27",
    "hizb": "53.25",
    "asbab": "0",
    "audio": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/4762",
    "theme": "1015",
    "text": "Allah menganugerahkan karunia kepada kami dan menjaga kami dari azab neraka.",
    "notes": null,
    "audio_partial": "{\"01\":\"https://cdn.equran.id/audio-partial/Abdullah-Al-Juhany/052027.mp3\",\"02\":\"https://cdn.equran.id/audio-partial/Abdul-Muhsin-Al-Qasim/052027.mp3\",\"03\":\"https://cdn.equran.id/audio-partial/Abdurrahman-as-Sudais/052027.mp3\",\"04\":\"https://cdn.equran.id/audio-partial/Ibrahim-Al-Dossari/052027.mp3\",\"05\":\"https://cdn.equran.id/audio-partial/Misyari-Rasyid-Al-Afasi/052027.mp3\",\"06\":\"https://cdn.equran.id/audio-partial/Yasser-Al-Dosari/052027.mp3\"}"
  },
  {
    "id": "4763",
    "surah": "52",
    "ayah": "28",
    "arab": "اِنَّا كُنَّا مِنْ قَبْلُ نَدْعُوْهُۗ اِنَّهٗ هُوَ الْبَرُّ الرَّحِيْمُ ࣖ",
