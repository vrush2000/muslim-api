  },
  {
    "id": "4766",
    "surah": "52",
    "ayah": "31",
    "arab": "قُلْ تَرَبَّصُوْا فَاِنِّيْ مَعَكُمْ مِّنَ الْمُتَرَبِّصِيْنَۗ",
