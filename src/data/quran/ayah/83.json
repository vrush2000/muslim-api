    "juz": "30",
    "hizb": "59.5",
    "asbab": "0",
    "audio": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/5871",
    "theme": null,
    "text": "Mereka (duduk) di atas dipan-dipan (sambil) melepas pandangan.",
