[
  {
    "id": "5830",
    "surah": "82",
    "ayah": "1",
    "arab": "اِذَا السَّمَاۤءُ انْفَطَرَتْۙ",
    "latin": "Iżas-samā'unfaṭarat.",
    "page": "587",
    "juz": "30",
    "hizb": "59.5",
    "asbab": "0",
    "audio": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/5830",
    "theme": null,
    "text": "Apabila langit terbelah,",
    "notes": null,
