  },
  {
    "id": "5831",
    "surah": "82",
    "ayah": "2",
    "arab": "وَاِذَا الْكَوَاكِبُ انْتَثَرَتْۙ",
    "latin": "Wa iżal-kawākibuntaṡarat.",
    "page": "587",
    "juz": "30",
    "hizb": "59.5",
    "asbab": "0",
    "audio": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/5831",
    "theme": null,
    "text": "apabila bintang-bintang jatuh berserakan,",
    "notes": null,
