    "page": "571",
    "juz": "29",
    "hizb": "57.75",
    "asbab": "0",
    "audio": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/5438",
    "theme": "1095",
    "text": "Allah menjadikan bumi untukmu sebagai hamparan",
    "notes": null,
    "audio_partial": "{\"01\":\"https://cdn.equran.id/audio-partial/Abdullah-Al-Juhany/071019.mp3\",\"02\":\"https://cdn.equran.id/audio-partial/Abdul-Muhsin-Al-Qasim/071019.mp3\",\"03\":\"https://cdn.equran.id/audio-partial/Abdurrahman-as-Sudais/071019.mp3\",\"04\":\"https://cdn.equran.id/audio-partial/Ibrahim-Al-Dossari/071019.mp3\",\"05\":\"https://cdn.equran.id/audio-partial/Misyari-Rasyid-Al-Afasi/071019.mp3\",\"06\":\"https://cdn.equran.id/audio-partial/Yasser-Al-Dosari/071019.mp3\"}"
  },
  {
    "id": "5439",
    "surah": "71",
    "ayah": "20",
    "arab": "لِّتَسْلُكُوْا مِنْهَا سُبُلًا فِجَاجًا ࣖ",
