  },
  {
    "id": "6068",
    "surah": "92",
    "ayah": "10",
    "arab": "فَسَنُيَسِّرُهٗ لِلْعُسْرٰىۗ",
    "latin": "Fa sanuyassiruhū lil-‘usrā.   ",
    "page": "595",
    "juz": "30",
    "hizb": "60.25",
    "asbab": "0",
    "audio": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6068",
    "theme": null,
    "text": "Kami akan memudahkannya menuju jalan kesengsaraan.",
