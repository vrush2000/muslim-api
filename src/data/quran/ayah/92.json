    "page": "595",
    "juz": "30",
    "hizb": "60.25",
    "asbab": "0",
    "audio": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6067",
    "theme": null,
    "text": "serta mendustakan (balasan) yang terbaik,",
    "notes": null,
