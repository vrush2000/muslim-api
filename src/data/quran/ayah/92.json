    "page": "595",
    "juz": "30",
    "hizb": "60.25",
    "asbab": "0",
    "audio": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6071",
    "theme": null,
    "text": "Sesungguhnya milik Kamilah akhirat dan dunia.",
    "notes": null,
    "audio_partial": "{\"01\":\"https://cdn.equran.id/audio-partial/Abdullah-Al-Juhany/092013.mp3\",\"02\":\"https://cdn.equran.id/audio-partial/Abdul-Muhsin-Al-Qasim/092013.mp3\",\"03\":\"https://cdn.equran.id/audio-partial/Abdurrahman-as-Sudais/092013.mp3\",\"04\":\"https://cdn.equran.id/audio-partial/Ibrahim-Al-Dossari/092013.mp3\",\"05\":\"https://cdn.equran.id/audio-partial/Misyari-Rasyid-Al-Afasi/092013.mp3\",\"06\":\"https://cdn.equran.id/audio-partial/Yasser-Al-Dosari/092013.mp3\"}"
  },
  {
    "id": "6072",
    "surah": "92",
    "ayah": "14",
    "arab": "فَاَنْذَرْتُكُمْ نَارًا تَلَظّٰىۚ",
