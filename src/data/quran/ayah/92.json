    "latin": "Wa sayujannabuhal-atqā.",
    "page": "596",
    "juz": "30",
    "hizb": "60.25",
    "asbab": "0",
    "audio": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6075",
    "theme": null,
    "text": "Akan dijauhkan darinya (neraka) orang yang paling bertakwa,",
    "notes": null,
    "audio_partial": "{\"01\":\"https://cdn.equran.id/audio-partial/Abdullah-Al-Juhany/092017.mp3\",\"02\":\"https://cdn.equran.id/audio-partial/Abdul-Muhsin-Al-Qasim/092017.mp3\",\"03\":\"https://cdn.equran.id/audio-partial/Abdurrahman-as-Sudais/092017.mp3\",\"04\":\"https://cdn.equran.id/audio-partial/Ibrahim-Al-Dossari/092017.mp3\",\"05\":\"https://cdn.equran.id/audio-partial/Misyari-Rasyid-Al-Afasi/092017.mp3\",\"06\":\"https://cdn.equran.id/audio-partial/Yasser-Al-Dosari/092017.mp3\"}"
  },
  {
    "id": "6076",
    "surah": "92",
    "ayah": "18",
