    "page": "595",
    "juz": "30",
    "hizb": "60.25",
    "asbab": "0",
    "audio": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6063",
    "theme": null,
    "text": "Siapa yang memberikan (hartanya di jalan Allah) dan bertakwa",
