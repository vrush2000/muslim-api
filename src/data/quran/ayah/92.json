    "page": "595",
    "juz": "30",
    "hizb": "60.25",
    "asbab": "0",
    "audio": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/6062",
    "theme": null,
    "text": "sesungguhnya usahamu benar-benar beraneka ragam.",
    "notes": null,
